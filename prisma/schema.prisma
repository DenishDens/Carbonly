
// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Material {
  id          String   @id @default(uuid())
  name        String
  code        String
  description String
  category    String
  supplier    String?
  dateAdded   String
  lastUpdated String
  isUserAdded Boolean  @default(false)
}
